<toolchains>
    <toolchain>
        <type>jdk</type>
        <provides>
            <id>Java8</id>
            <version>1.8</version>
        </provides>
        <configuration>
            <jdkHome>${env.JAVA_HOME}</jdkHome>
        </configuration>
    </toolchain>
    <toolchain>
        <type>jdk</type>
        <provides>
            <id>Java2</id>
            <version>1.2</version>
        </provides>
        <configuration>
            <jdkHome>${env.JAVA_HOME_JDK2}</jdkHome>
        </configuration>
    </toolchain>
</toolchains>